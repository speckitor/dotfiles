syntax on
set tabstop=4
set shiftwidth=4
set expandtab
autocmd FileType make setlocal noexpandtab
set ignorecase
set smartcase
set number
set relativenumber
set guioptions -=m
set guioptions -=T
set cursorline
set noesckeys
set incsearch
set modeline
set hidden
set path+=**
set autochdir
set clipboard=unnamedplus
set termguicolors
colorscheme elflord
